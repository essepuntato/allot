    <!-- https://w3id.org/akn/ontology/allot/Concept -->

    <owl:Class rdf:about="https://w3id.org/akn/ontology/allot/Concept">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000002"/>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObject"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/akn/ontology/allot/Reference"/>
