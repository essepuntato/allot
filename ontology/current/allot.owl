    <!-- https://w3id.org/akn/ontology/allot/hasEmbodiment -->

    <owl:ObjectProperty rdf:about="https://w3id.org/akn/ontology/allot/hasEmbodiment">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/akn/ontology/allot/hasRelatedReference"/>
        <rdfs:domain rdf:resource="https://w3id.org/akn/ontology/allot/FRBRExpression"/>
        <rdfs:range rdf:resource="https://w3id.org/akn/ontology/allot/FRBRManifestation"/>
        <rdfs:comment xml:lang="en">This property allows one to specify the manifestation in which an expression is embodied.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/akn/ontology/allot"/>
        <rdfs:label xml:lang="en">has embodiment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/akn/ontology/allot/hasExemplar -->

    <owl:ObjectProperty rdf:about="https://w3id.org/akn/ontology/allot/hasExemplar">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/akn/ontology/allot/hasRelatedReference"/>
        <rdfs:domain rdf:resource="https://w3id.org/akn/ontology/allot/FRBRManifestation"/>
        <rdfs:range rdf:resource="https://w3id.org/akn/ontology/allot/FRBRItem"/>
        <rdfs:comment xml:lang="en">This property allows one to specify an exemplification of a manifestation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/akn/ontology/allot"/>
        <rdfs:label xml:lang="en">has exemplar</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/akn/ontology/allot/hasRealization -->

    <owl:ObjectProperty rdf:about="https://w3id.org/akn/ontology/allot/hasRealization">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/akn/ontology/allot/hasRelatedReference"/>
        <rdfs:domain rdf:resource="https://w3id.org/akn/ontology/allot/FRBRWork"/>
        <rdfs:range rdf:resource="https://w3id.org/akn/ontology/allot/FRBRExpression"/>
        <rdfs:comment xml:lang="en">This property allows one to specify the entities that realizes a work.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/akn/ontology/allot"/>
        <rdfs:label xml:lang="en">has realization</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/akn/ontology/allot/hasRelatedReference -->

    <owl:ObjectProperty rdf:about="https://w3id.org/akn/ontology/allot/hasRelatedReference">
        <rdfs:domain rdf:resource="https://w3id.org/akn/ontology/allot/Reference"/>
        <rdfs:range rdf:resource="https://w3id.org/akn/ontology/allot/Reference"/>
        <rdfs:comment xml:lang="en">This property allows one to link two instances of the class Reference that are related for some reason.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/akn/ontology/allot"/>
        <rdfs:label xml:lang="en">has related reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/akn/ontology/allot/mentions -->

    <owl:ObjectProperty rdf:about="https://w3id.org/akn/ontology/allot/mentions">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/akn/ontology/allot/hasRelatedReference"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/akn/ontology/allot/FRBRExpression"/>
                    <rdf:Description rdf:about="https://w3id.org/akn/ontology/allot/FRBRItem"/>
                    <rdf:Description rdf:about="https://w3id.org/akn/ontology/allot/FRBRManifestation"/>
                    <rdf:Description rdf:about="https://w3id.org/akn/ontology/allot/FRBRWork"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">This property allows to specify the references that are mentioned by a document, independently from the particular FRBR level one is considering.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/akn/ontology/allot"/>
        <rdfs:label xml:lang="en">mentions</rdfs:label>
    </owl:ObjectProperty>
    


